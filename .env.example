# Shopify App Configuration
# Copy this file to .env and fill in your actual values

# Get these from your Shopify Partner Dashboard
SHOPIFY_API_KEY=your_api_key_here
SHOPIFY_API_SECRET=your_api_secret_here

# Your app's URL (use ngrok or similar for local development)
SHOPIFY_APP_URL=https://your-app-url.ngrok.io

# Port for local development
PORT=3000

# Development store URL (optional)
SHOPIFY_DEV_STORE=your-dev-store.myshopify.com